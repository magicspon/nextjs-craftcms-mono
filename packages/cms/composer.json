{
	"require": {
		"craftcms/cms": "^3.6.4",
		"craftcms/redactor": "^2.8",
		"craftcms/webhooks": "^2.3",
		"nystudio107/craft-seomatic": "3.3.44",
		"sebastianlenz/linkfield": "2.0.0-beta.12",
		"spicyweb/craft-embedded-assets": "^2.7",
		"studioespresso/craft-splashingimages": "^2.1",
		"verbb/formie": "1.4.9",
		"verbb/super-table": "^2.6",
		"vlucas/phpdotenv": "^3.4.0"
	},
	"require-dev": {
		"yiisoft/yii2-shell": "^2.0.3"
	},
	"autoload": {
		"psr-4": {
			"modules\\": "modules/"
		}
	},
	"config": {
		"sort-packages": true,
		"optimize-autoloader": true,
		"platform": {
			"php": "7.4.11"
		}
	},
	"scripts": {
		"post-root-package-install": [
			"@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
		]
	}
}
