{
	"name": "@spon/frontend",
	"version": "0.6.0",
	"scripts": {
		"prewrite:data": "tsc scripts/writeGlobalDataToDisk.ts --outDir scripts/dist --types node --module commonjs --target ES2017 --esModuleInterop",
		"write:data": "node ./scripts/dist/scripts/writeGlobalDataToDisk.js",
		"postwrite:data": "rm -rf ./scripts/dist",
		"dev": "yarn write:data; next dev",
		"build": "yarn write:data; next build",
		"start": "next start",
		"lint": "eslint --ext ts,js,tsx,jsx",
		"lint-css": "stylelint ./**/*.css --quiet --fix",
		"prettier": "prettier --ignore-path .gitignore \"**/*.+(js|ts|tsx)\"",
		"format": "prettier --write \"src/**/*.{ts,tsx,json,js,jsx}\"",
		"check-format": "yarn prettier -- --list-different",
		"test": "jest",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook"
	},
	"dependencies": {
		"cross-env": "7.0.3",
		"graphql": "^15.5.1",
		"graphql-request": "^3.4.0",
		"next": "11.0.1",
		"ramda": "^0.27.1",
		"react": "17.0.2",
		"react-dom": "17.0.2"
	},
	"private": true,
	"devDependencies": {
		"@babel/core": "^7.14.6",
		"@storybook/addon-actions": "^6.3.2",
		"@storybook/addon-essentials": "^6.3.2",
		"@storybook/addon-links": "^6.3.2",
		"@storybook/addon-postcss": "^2.0.0",
		"@storybook/react": "^6.3.2",
		"@svgr/webpack": "^5.5.0",
		"@testing-library/jest-dom": "^5.14.1",
		"@testing-library/react": "^12.0.0",
		"@types/graphql": "^14.5.0",
		"@types/node": "^16.0.0",
		"@types/ramda": "^0.27.42",
		"@types/react": "^17.0.13",
		"@types/react-dom": "17.0.8",
		"autoprefixer": "^10.2.6",
		"babel-loader": "^8.2.2",
		"eslint-config-next": "^11.0.1",
		"postcss": "^8.3.5",
		"postcss-preset-env": "^6.7.0",
		"tailwindcss": "^2.2.4",
		"ts-node": "10.0.0",
		"tsconfig-paths-webpack-plugin": "^3.5.1",
		"typescript": "^4.3.5"
	},
	"license": "MIT"
}
